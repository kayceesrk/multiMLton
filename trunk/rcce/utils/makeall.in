if [ -f ./common/symbols ]; then
  for flags in 0 1; do 
    for pwr in _INSERT_POWERPARS_INSERT_; do 
      for api in gory nongory; do 
        make PWRMGMT=$pwr API=$api SINGLEBITFLAGS=$flags
      done 
    done
  done
  _INSERT_MAKE_MPB_
